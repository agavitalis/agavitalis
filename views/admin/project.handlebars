    <!-- Contact Section -->
    <section class="section contact-section dark" id="contact">
        <div class="container mt-5">
            <div class="row justify-content-center mb-5 pb-3">
                <div class="col-md-7 heading-section  text-center">
                    <div class=" white section-title-uin"> Projects </div>
                    <h2 class="mb-4">Manage projects</h2>
                    <p class="flip"><span class="deg1"></span><span class="deg2"></span><span class="deg3"></span></p>
                    <p class="mt-5">Add and manage your projects here.</p>
                </div>
            </div>
            <div class="row block-9">
                <div class="col-md-4">
                    <form action="#" class="contact-form">

                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Job Title">
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Duration">
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Roles Title">
                        </div>
                        <div class="form-group">
                            <textarea name="message" id="message" cols="30" rows="4" class="form-control"
                                placeholder="Role Description"></textarea>
                        </div>
                        <div class="form-group">
                            <input type="Register" value="Send Message" class="btn btn-primary py-3 px-5">
                        </div>
                    </form>
                </div>
                <div class="col-md-8">               
                    <table class="table table-bordered table-dark">
                        <thead>
                            <tr>
                            <th scope="col">#</th>
                            <th scope="col">First</th>
                            <th scope="col">Last</th>
                            <th scope="col">Handle</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>@mdo</td>
                            </tr>
                            <tr>
                            <th scope="row">2</th>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>@fat</td>
                            </tr>
                            <tr>
                            <th scope="row">3</th>
